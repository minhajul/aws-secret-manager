{
    "name": "minhajul/aws-secret-manager",
    "description": "Laravel implementation for AWS Secret Manager",
    "keywords": [
        "minhajul",
        "laravel",
        "aws-secret-manager"
    ],
    "homepage": "https://github.com/minhajul/aws-secret-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Md Minhajul Islam",
            "email": "kmsohelinfo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.271",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Minhajul\\AwsSecretManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minhajul\\AwsSecretManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minhajul\\AwsSecretManager\\AwsSecretManagerServiceProvider"
            ],
            "aliases": {
                "AwsSecretManager": "Minhajul\\AwsSecretManager\\Facades\\AwsSecretManager"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
