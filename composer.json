{
    "name": "minhajul/aws-secret-manager",
    "description": "This is my package aws-secret-manager",
    "keywords": [
        "minhajul",
        "laravel",
        "aws-secret-manager"
    ],
    "homepage": "https://github.com/minhajul/aws-secret-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Md Minhajul Islam",
            "email": "kmsohelinfo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0",
        "aws/aws-sdk-php": "^3.271",
        "ext-json": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Minhajul\\AwsSecretManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minhajul\\AwsSecretManager\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minhajul\\AwsSecretManager\\AwsSecretManagerServiceProvider"
            ],
            "aliases": {
                "AwsSecretManager": "Minhajul\\AwsSecretManager\\Facades\\AwsSecretManager"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
